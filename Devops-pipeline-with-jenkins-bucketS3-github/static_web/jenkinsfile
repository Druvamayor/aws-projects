pipeline {
     agent any
     stages {
         stage('Build') {
             steps {
                 sh 'echo "Hello World"'
                 sh '''
                     echo "Multiline shell steps works too"
                     ls -lah
                 '''
             }
         }      
         stage('Upload') {

        dir('path/to/your/project/workspace'){

            pwd(); //Log current directory

            withAWS(region:'us-east-1',credentials:'jenkins-user-credentials-for-aws-s3') {

                 def identity=awsIdentity();//Log AWS credentials

                // Upload files from working directory 'static_web' in your project workspace
                s3Upload(bucket:"veggycommons.com", workingDir:'static_web', includePathPattern:'**/*');
            }

        };
    }



     }
}

